{
    "name": "委托下单",
    "url" : "http://trade-route-develop.ntdev.be:8080/GXSocket/NewGateWay.aspx",
    "method" : "post",
    "type" : "form-data",
    "headers" : {
        "Content-Type":"application/x-www-form-urlencoded",
        "User-Agent":"Openwave",
        "Content-Language":"en-US"
    }, 
    "body" : [
        {
            "key": "account",
            "type": "string",
            "lenght": 16,
            "isempty": "Y", 
            "valueArea" : null,
            "default" : "3001331110"
        },
        {
            "key": "password ",
            "type": "string",
            "lenght": 32,
            "isempty": "Y",
            "valueArea" : null, 
            "default" : "abcd1234"
        },
        {
            "key": "account_type",
            "type": "string",
            "lenght": 4,
            "isempty": "Y",
            "valueArea" : ["0", "1", "2", "3", "4", "5", "6", "7", "8"],
            "default" : "8"
        }, 
        
    ],
    "params" : {"QueryString" : "%(params)"}

}

